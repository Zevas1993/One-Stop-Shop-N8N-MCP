{
  "name": "n8n-mcp",
  "version": "3.0.0",
  "description": "n8n Co-Pilot - Stateless MCP server with live node sync, bulletproof validation, and embedded LLM",
  "main": "dist/main.js",
  "bin": {
    "n8n-mcp": "./dist/main.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "go": "node start.js",
    "go:http": "node start.js --http",
    "build": "tsc -p tsconfig.build.json",
    "build:full": "tsc",
    "build:fast": "tsc -p tsconfig.build.json --skipLibCheck",
    "start": "node start.js",
    "start:http": "node start.js --http",
    "start:mcp": "node start.js",
    "start:direct": "node dist/main.js",
    "dev": "npm run build && npm run start",
    "dev:http": "npm run build && npm run start:http",
    "dev:quick": "ts-node --transpile-only src/main.ts --http",
    "dev:mcp": "ts-node --transpile-only src/main.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec 'npm run build && npm run start:http'",
    "docker:build": "docker build -t n8n-mcp:latest .",
    "docker:run": "docker run -it --rm -e N8N_API_URL -e N8N_API_KEY n8n-mcp:latest",
    "docker:compose": "docker compose up -d",
    "docker:compose:down": "docker compose down",
    "setup": "node setup.js",
    "setup:claude-desktop": "node setup.js --mode=claude-desktop",
    "setup:http": "node setup.js --mode=http",
    "setup:docker": "node setup.js --mode=docker",
    "rebuild": "node dist/scripts/rebuild-github.js",
    "rebuild:local": "node dist/scripts/rebuild.js",
    "rebuild:optimized": "node dist/scripts/rebuild-optimized.js",
    "rebuild:github": "node dist/scripts/rebuild-github.js",
    "validate": "node dist/scripts/validate.js",
    "test": "jest",
    "test:core": "jest --testPathPattern=core",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "legacy:start": "node dist/mcp/index.js",
    "legacy:http": "MCP_MODE=http node dist/mcp/index.js",
    "legacy:consolidated": "node dist/consolidated-server.js",
    "test-nodes": "node dist/scripts/test-nodes.js",
    "test:single-session": "./scripts/test-single-session.sh",
    "update:n8n": "node scripts/update-n8n-deps.js",
    "update:n8n:check": "node scripts/update-n8n-deps.js --dry-run",
    "fetch:templates": "node dist/scripts/fetch-templates.js",
    "fetch:templates:robust": "node dist/scripts/fetch-templates-robust.js",
    "test:templates": "node dist/scripts/test-templates.js",
    "test:workflow-validation": "node dist/scripts/test-workflow-validation.js",
    "test:template-validation": "node dist/scripts/test-template-validation.js",
    "test:essentials": "node dist/scripts/test-essentials.js",
    "test:enhanced-validation": "node dist/scripts/test-enhanced-validation.js",
    "test:ai-workflow-validation": "node dist/scripts/test-ai-workflow-validation.js",
    "test:mcp-tools": "node dist/scripts/test-mcp-tools.js",
    "test:nano-llm": "npx tsx scripts/test-nano-llm-integration.ts",
    "test:n8n-manager": "node dist/scripts/test-n8n-manager-integration.js",
    "test:n8n-validate-workflow": "node dist/scripts/test-n8n-validate-workflow.js",
    "test:typeversion-validation": "node dist/scripts/test-typeversion-validation.js",
    "test:parameter-validation": "node dist/scripts/test-parameter-validation.js",
    "test:workflow-diff": "node dist/scripts/test-workflow-diff.js",
    "test:github": "node dist/scripts/test-github-integration.js",
    "test:transactional-diff": "node dist/scripts/test-transactional-diff.js",
    "test:mcp:update-partial": "node dist/scripts/test-mcp-n8n-update-partial.js",
    "test:update-partial:debug": "node dist/scripts/test-update-partial-debug.js",
    "test:v3": "node dist/scripts/test-v3-implementation.js",
    "test:v3:runtime": "node scripts/test-v3-runtime.js",
    "update:outlook-workflow": "node dist/scripts/update-outlook-workflow.js",
    "benchmark": "node scripts/benchmark.js",
    "dashboard": "node dist/dashboard/dashboard-server.js",
    "dashboard:dev": "nodemon --watch src/dashboard --ext ts --exec 'npm run build && npm run dashboard'",
    "db:rebuild": "node dist/scripts/rebuild-database.js",
    "db:init": "node -e \"new (require('./dist/services/sqlite-storage-service').SQLiteStorageService)(); console.log('Database initialized')\"",
    "docs:rebuild": "ts-node src/scripts/rebuild-database.ts",
    "sync:runtime-version": "node scripts/sync-runtime-version.js",
    "test:graphrag": "ts-node src/scripts/test-graphrag.ts",
    "test:http-client": "ts-node src/scripts/test-http-mcp-client.ts",
    "seed:catalog": "ts-node src/scripts/seed-graph-catalog.ts",
    "metrics:snapshot": "ts-node src/scripts/metrics-snapshot.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zevas1993/One-Stop-Shop-N8N-MCP.git"
  },
  "keywords": [
    "n8n",
    "mcp",
    "model-context-protocol",
    "ai",
    "workflow",
    "automation",
    "copilot",
    "llm"
  ],
  "author": "Romuald Czlonkowski @ www.aiadvisors.pl/en",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zevas1993/One-Stop-Shop-N8N-MCP/issues"
  },
  "homepage": "https://github.com/Zevas1993/One-Stop-Shop-N8N-MCP#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@n8n/n8n-nodes-langchain": "1.120.2",
    "@octokit/rest": "^22.0.0",
    "@playwright/test": "^1.53.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/node-cron": "^3.0.11",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "n8n": "1.121.3",
    "n8n-core": "1.120.2",
    "n8n-workflow": "1.118.2",
    "node-cron": "^4.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "axios": "^1.10.0",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "sql.js": "^1.13.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
